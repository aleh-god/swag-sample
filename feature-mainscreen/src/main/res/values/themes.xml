<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Swagger" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primarySwaggerBrand</item>
        <item name="colorPrimaryVariant">@color/primaryVariantSwaggerBrand</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="backgroundColor">@color/backgroundSwaggerBrand</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/primarySwaggerBrand</item>
        <item name="colorSecondaryVariant">@color/primaryVariantSwaggerBrand</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>